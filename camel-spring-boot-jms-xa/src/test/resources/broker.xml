<configuration xmlns="urn:activemq" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:activemq artemis-configuration.xsd">
    <core xmlns="urn:activemq:core">
        <max-disk-usage>95</max-disk-usage>
        <security-enabled>false</security-enabled>
        <persistence-enabled>false</persistence-enabled>
        <acceptors>
            <acceptor name="netty">tcp://localhost:61616</acceptor>
        </acceptors>
    </core>
</configuration>
